.goods-list {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  padding: 16rpx;
}
.goods-list .item {
  width: 49%;
  background-color: #fff;
  margin-bottom: 16rpx;
  padding-bottom: 16rpx;
  text-align: center;
}
.goods-list .item .image {
  width: 100%;
}
.goods-list .item .price {
  color: darkred;
  margin-top: 16rpx;
  margin-bottom: 16rpx;
}
